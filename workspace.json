{
  "version": 1,
  "projects": {
    "dicom-viewer": {
      "root": "apps/dicom-viewer",
      "sourceRoot": "apps/dicom-viewer/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/dicom-viewer",
            "index": "apps/dicom-viewer/src/index.html",
            "main": "apps/dicom-viewer/src/main.ts",
            "polyfills": "apps/dicom-viewer/src/polyfills.ts",
            "tsConfig": "apps/dicom-viewer/tsconfig.app.json",
            "assets": [
              "apps/dicom-viewer/src/favicon.ico",
              "apps/dicom-viewer/src/assets"
            ],
            "styles": ["apps/dicom-viewer/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/dicom-viewer/src/environments/environment.ts",
                  "with": "apps/dicom-viewer/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "dicom-viewer:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "dicom-viewer:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/dicom-viewer/tsconfig.app.json",
              "apps/dicom-viewer/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/dicom-viewer/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/dicom-viewer/jest.config.js",
            "tsConfig": "apps/dicom-viewer/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/dicom-viewer/src/test-setup.ts"
          }
        }
      }
    },
    "dicom-viewer-e2e": {
      "root": "apps/dicom-viewer-e2e",
      "sourceRoot": "apps/dicom-viewer-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dicom-viewer-e2e/cypress.json",
            "tsConfig": "apps/dicom-viewer-e2e/tsconfig.e2e.json",
            "devServerTarget": "dicom-viewer:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dicom-viewer:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/dicom-viewer-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/dicom-viewer-e2e/**/*"]
          }
        }
      }
    },
    "ng-dicom-viewer": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ng-dicom-viewer",
      "sourceRoot": "apps/ng-dicom-viewer/src",
      "prefix": "webade-web-components",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ng-dicom-viewer",
            "index": "apps/ng-dicom-viewer/src/index.html",
            "main": "apps/ng-dicom-viewer/src/main.ts",
            "polyfills": "apps/ng-dicom-viewer/src/polyfills.ts",
            "tsConfig": "apps/ng-dicom-viewer/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ng-dicom-viewer/src/favicon.ico",
              "apps/ng-dicom-viewer/src/assets"
            ],
            "styles": ["apps/ng-dicom-viewer/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-dicom-viewer/src/environments/environment.ts",
                  "with": "apps/ng-dicom-viewer/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-dicom-viewer:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-dicom-viewer:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-dicom-viewer:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng-dicom-viewer/tsconfig.app.json",
              "apps/ng-dicom-viewer/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ng-dicom-viewer/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ng-dicom-viewer/jest.config.js",
            "tsConfig": "apps/ng-dicom-viewer/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/ng-dicom-viewer/src/test-setup.ts"
          }
        }
      }
    },
    "ng-dicom-viewer-e2e": {
      "root": "apps/ng-dicom-viewer-e2e",
      "sourceRoot": "apps/ng-dicom-viewer-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-dicom-viewer-e2e/cypress.json",
            "tsConfig": "apps/ng-dicom-viewer-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng-dicom-viewer:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-dicom-viewer:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ng-dicom-viewer-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/ng-dicom-viewer-e2e/**/*"]
          }
        }
      }
    },
    "core-containers": {
      "root": "libs/core-containers",
      "sourceRoot": "libs/core-containers/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/core-containers/tsconfig.lib.json",
              "libs/core-containers/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core-containers/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core-containers/jest.config.js",
            "tsConfig": "libs/core-containers/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-services": {
      "root": "libs/core-services",
      "sourceRoot": "libs/core-services/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/core-services/tsconfig.lib.json",
              "libs/core-services/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core-services/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core-services/jest.config.js",
            "tsConfig": "libs/core-services/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "ng-dicom-viewer",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
